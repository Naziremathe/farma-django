{
  "builds": [
    {
      "src": "fama/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "fama/wsgi.py"
    }
  ],
  "functions": {
    "fama/wsgi.py": {
      "maxDuration": 30,
      "memory": 3008
    }
  }
}